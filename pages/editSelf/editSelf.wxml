<view class="container_up">
<view class="character_show">
<image class="personal_body"></image>
<image class="personal_hair" src="{{personalImage.hairStyle}}"></image>
<image class="personal_face" src="{{personalImage.faceStyle}}"></image>
<image class="personal_overcoat" src="{{personalImage.overcoatStyle}}"></image>
<image class="personal_blouse" src="{{personalImage.blouseStyle}}"></image>
<image class="personal_trouser" src="{{personalImage.trouserStyle}}"></image>
<image class="personal_shoes" src="{{personalImage.shoesStyle}}"></image>
<image class="personal_glasses" src="{{personalImage.glassesStyle}}"></image>
</view>
</view>
<view>
<view class="btn_save_img">保存</view>
</view>
<view class="container_down">
<view  class="tool_type">
<i class="iconfont iconface_id item" bindtap="changeItemList" data-type="feature" style="color:{{currentStyle=='feature'?'#009F7E':'#B8B8BA'}}"></i>
<i class="iconfont iconface item" bindtap="changeItemList" data-type="hair" style="color:{{currentStyle=='hair'?'#009F7E':'#B8B8BA'}}"></i>
<i class="iconfont iconsmile item" bindtap="changeItemList" data-type="expression" style="color:{{currentStyle=='expression'?'#009F7E':'#B8B8BA'}};font-size:48rpx;"></i>
<i class="iconfont iconwaitao item" bindtap="changeItemList" data-type="overcoat"  style="color:{{currentStyle=='overcoat'?'#009F7E':'#B8B8BA'}};"></i>
<i class="iconfont iconyifu item"  bindtap="changeItemList" data-type="shirt" style="color:{{currentStyle=='shirt'?'#009F7E':'#B8B8BA'}};font-size:54rpx;"></i>
<i class="iconfont iconkuzi item" bindtap="changeItemList" data-type="trousers"style="color:{{currentStyle=='trousers'?'#009F7E':'#B8B8BA'}};font-size:50rpx;"></i>
<i class="iconfont iconshoes item" bindtap="changeItemList" data-type="shoes" style="color:{{currentStyle=='shoes'?'#009F7E':'#B8B8BA'}};font-size:60rpx;"></i>
<i class="iconfont iconyanjing item"  bindtap="changeItemList" data-type="glasses" style="color:{{currentStyle=='glasses'?'#009F7E':'#B8B8BA'}};font-size:54rpx;"></i>
<i class="iconfont iconicon-test item"  bindtap="changeItemList" data-type="others" style="color:{{currentStyle=='others'?'#009F7E':'#B8B8BA'}};font-size:48rpx;"></i>
</view>

<!-- 
current_style:feature
--> 
<view class="tool_items" wx:if="{{currentStyle=='feature'}}">
<view class="btn_goto_photo" bindtap="btn_goto_shoot">
<i class="iconfont iconxiangji" style="color: #00906A;font-size:48rpx;"></i>
</view>
<block wx:for="{{toolitemList}}" wx:key="item">
<view bindtap="setStyle" class="single_item"></view>
</block>
</view>
